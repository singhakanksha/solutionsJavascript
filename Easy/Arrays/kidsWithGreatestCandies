var kidsWithCandies = function(candies, extraCandies) {
    max = Math.max(...candies)
    console.log(max)
    result = []
    for(let i=0; i<candies.length; i++){
        if( (candies[i] + extraCandies) >= max){
            result.push(true)
        }else{
            result.push(false)
        }
     }
     return result
};


console.log(kidsWithCandies([2,3,5,1,3],3))
